{
  "name": "addins",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.1.0",
    "axios": "^0.18.0",
    "moment": "^2.22.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "nuka-carousel": "^3.0.0",
    "react": "^16.4.1",
    "react-alice-carousel": "^1.8.0",
    "react-bootstrap": "^0.32.0",
    "react-dom": "^16.2.0",
    "react-google-maps": "^9.4.5",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/assets/scss -o src/assets/css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/assets/scss -o src/assets/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6"
  }
}
